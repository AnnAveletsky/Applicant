@model IEnumerable<Applicant.Models.Tag>

@{
    ViewBag.Title = "Index";
}

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.TagName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.TagName)
        </td>
        <td>
            @Html.ActionLink("Подробнее", "Details", new { id=item.TagId }) 
        </td>
    </tr>
}

</table>
